{
  "name": "c2c_images",
  "version": "1.4.1",
  "type": "module",
  "scripts": {
    "start": "node ./dist/server.js",
    "build": "tsc",
    "lint": "eslint --fix src/ test/",
    "lint:ci": "eslint src/ test/ -f junit -o ./reports/junit/eslint-results.xml",
    "test": "npm run test:unit",
    "test:ci": "npm run test:ci:unit && npm run test:ci:s3",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:ci:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.ci.mjs --runInBand",
    "test:s3": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.s3.mjs --runInBand",
    "test:ci:s3": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.s3.ci.mjs --runInBand",
    "clean:incoming": "node ./dist/scripts/clean.js",
    "prepare": "husky install",
    "preversion": "npm run build && npm run lint && npm run test",
    "postversion": "git push --follow-tags --no-verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c2corg/c2c_images.git"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/c2corg/c2c_images/issues",
    "email": "dev@camptocamp.org"
  },
  "homepage": "https://www.camptocamp.org",
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.1.2",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.8.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "image-size": "^1.0.2",
    "jest": "^29.2.0",
    "jest-junit": "^14.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "aws-sdk": "^2.1231.0",
    "command-exists": "^1.2.9",
    "cross-spawn": "^7.0.3",
    "debug": "^4.3.4",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "prom-client": "^14.1.0",
    "sanitize-filename": "^1.6.3"
  }
}
